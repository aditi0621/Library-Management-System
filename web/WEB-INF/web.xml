<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Alogin</servlet-name>
        <servlet-class>com.Alogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>com.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Ulogin1</servlet-name>
        <servlet-class>com.Ulogin1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addToBag</servlet-name>
        <servlet-class>com.addToBag</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BookReturn</servlet-name>
        <servlet-class>com.BookReturn</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Alogin</servlet-name>
        <url-pattern>/Alogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Ulogin1</servlet-name>
        <url-pattern>/Ulogin1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addToBag</servlet-name>
        <url-pattern>/addToBag</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BookReturn</servlet-name>
        <url-pattern>/BookReturn</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
